package calculate_Ver4;

import lombok.Data;

@Data
public abstract class Is_A_Cal {
	
	public int a;
	public int b;
	public String op;
	public double sum;

	// main에서 전달받은 num을 a에 저장한다.
	public Is_A_Cal(int num) {
		this.a = num;
	}
	
	
	public String StringBuffer;
	public double result;
	
	public abstract void add();
	public abstract void sub();
	public abstract void mul();
	public abstract void div();
	public abstract void calculate(boolean flag);
	public abstract char judgeOp(String op);
	public abstract boolean judgeCal(boolean temp);
	public abstract void Calpath();
	
	
}
