package calculate_Ver4;

public abstract class CalAbstract {
	public int a;
	public int b;
	public String op;
	public int result;
	public int sum;
	
	public abstract void add();
	public abstract void sub();
	public abstract void mul();
	public abstract void div();
	public abstract void calculate(boolean flag);
	public abstract char judgeOp(String op);
	public abstract boolean judgeCal();
	
	
}
