package calculate_Ver_Adapter.copy;

import calculate_Ver_Interface.CalResult;

public class CalAdapter extends CalAbstract implements CalResult {
	
	CalAbstract calAbstract;
	
	class DefaultDisplay extends CalAdapter{}
	
	@Override
	public void cal() {
		// TODO Auto-generated method stub
		
	}
	

	@Override
	public void PrintProcess() {
		System.out.println("과정 : "+ getSb());
	}
	
	@Override
	public void PrintResult() {
		System.out.println("결과 : " + calAbstract.getResult());
	}
	
	public void PrintFinish() {
		System.out.println("계산끝났당오예~!");
	}
	
	public void Display() {
		if(calAbstract==null) {
			DefaultDisplay defaultDisplay = new DefaultDisplay();
			defaultDisplay.PrintProcess();
			defaultDisplay.PrintResult();
			defaultDisplay.PrintFinish();
		}
		else {
			System.out.println("ㅋㅋ");
		}
	}
	
	
	
}
