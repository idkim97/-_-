package newCalculate;

public class Cal {
	
	
	public int add(int a, int b) {
		return a+b;
	}
	
	public int sub(int a, int b) {
		return a-b;
	}
	
	public int mul(int a, int b) {
		return a*b;
	}
	
	public double div(double a, double b) {
		return a/b;
	}
	
	public char judgeOp(String op) {
		if(op.equals("+")) {
			return op.charAt(0);
		}
		else if(op.equals("-")) {
			return op.charAt(0);
		}
		else if(op.equals("*")) {
			return op.charAt(0);
		}
		else if(op.equals("/")) {
			return op.charAt(0);
		}
		
		return 'X';
	}
	
	public int judgeCal() {
		return 0;
	}
	
	
	
}
